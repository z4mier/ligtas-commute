{
  "name": "ligtascommute",
  "private": true,
  "version": "1.0.0",
  "workspaces": ["apps/*"],
  "scripts": {
    "dev:api": "npm --workspace apps/api run dev",
    "dev:web": "npm --workspace apps/web-admin run dev",
    "dev:driver": "npm --workspace apps/mobile-driver run start",
    "dev:commuter": "npm --workspace apps/mobile-commuter run start",
    "dev": "concurrently -k \"npm run dev:api\" \"npm run dev:web\""
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "typescript": "~5.9.2"
  },
  "overrides": {
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-web": "^0.21.0"
  }
}
