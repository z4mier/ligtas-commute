{
  "name": "ligtascommute",
  "private": true,
  "version": "1.0.0",
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev:api": "npm --workspace apps/api run dev",
    "dev:web": "npm --workspace apps/web-admin run dev",
    "dev:commuter": "npm --workspace apps/mobile-commuter run start",
    "dev": "concurrently -k \"npm run dev:api\" \"npm run dev:web\""
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/native-stack": "^7.3.26",
    "bcrypt": "^6.0.0",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.16.0"
  }
}
